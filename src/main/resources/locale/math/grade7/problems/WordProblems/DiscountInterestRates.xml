<problems>
	<problem>
		<questionline>You have CAD var1 in Savings account. The Savings account gives var2% interest per annum. What will be your interest at end of one year? </questionline>
		<variable>
			<variableName>var1</variableName>
			<variableValue>0</variableValue>
			<variableType>int</variableType>
			<lowerlimit>2000</lowerlimit>
			<upprlimit>5000</upprlimit>
		</variable>
		<variable>
			<variableName>var2</variableName>
			<variableValue>0</variableValue>
			<variableType>int</variableType>
			<lowerlimit>2</lowerlimit>
			<upprlimit>5</upprlimit>
		</variable>
		<Answer>
			<answerType>decimal</answerType>
			<displayableanswer>exp</displayableanswer>
			<answerLine>exp</answerLine>
		</Answer>
		<expression>var1*var2/100</expression>
